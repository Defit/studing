<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >





<suite name="Suite">
	<method-selectors>
		<method-selector>
			<selector-class name="fwk.NewAnnotationMethodSelector" 
			priority="15" />
		</method-selector>
	</method-selectors>
	
	<test name="econfig_first">
		<groups>
			<run>
				<include name="first" />
			</run>
		</groups>
		<packages>
			<package name="test.*" />
		</packages>
	</test>
	
	<test name="econfig_second">
		<groups>
			<run>
				<include name="second" />
			</run>
		</groups>
		<packages>
			<package name="test.*" />
		</packages>
	</test>
	
	<test name="econfig_third">
		<groups>
			<run>
				<include name="third" />
			</run>
		</groups>
		<packages>
			<package name="test.*" />
		</packages>
	</test>
</suite>



